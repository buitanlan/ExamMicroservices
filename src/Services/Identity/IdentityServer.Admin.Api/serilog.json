{
	"Serilog": {
		"LevelSwitches": { "$controlSwitch": "Information" },
		"MinimumLevel": { "ControlledBy": "$controlSwitch" },
		"WriteTo": [
			{
				"Name": "Console"
			},
			{
				"Name": "File",
				"Args": { "path":  "./logs/log-.txt", "rollingInterval": "Day" }
			},
			{
				"Name": "Seq",
				"Args": { "serverUrl":  "http://localhost:5341" }
			}
		]
	}
}